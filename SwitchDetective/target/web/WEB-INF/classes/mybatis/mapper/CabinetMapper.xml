<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssm.demo.mapper.CabinetMapper">

    <select id="getCabinetById" parameterType="String" resultType="cabinet">
        SELECT *
        FROM Cabinet
        WHERE id = #{id}
    </select>

</mapper>